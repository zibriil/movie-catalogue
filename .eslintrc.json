{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true,
    "codeceptjs/codeceptjs": true
  },
  "extends": ["airbnb-base", "plugin:codeceptjs/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "linebreak-style": "off",
    "operator-linebreak": "off",
    "arrow-parens": "off",
    "import/no-extraneous-dependencies": "off",
    "no-unused-expressions": "off",
    "object-curly-newline": "off",
    "no-new": "off",
    "no-shadow": "off",
    "comma-dangle": "off"
  },
  "plugins": ["jasmine", "codeceptjs"]
}
